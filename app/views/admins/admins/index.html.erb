  <div class="card-body">
      <div class="table-responsive">
        <table id="myTable" class="table table-bordered table-striped table-hover" width="100%" cellspacing="0">
          <thead>
            <tr>
              <th>ID</th>
              <th>E-mail</th>
              <th>Criado em</th>
              <th>Atualizado em</th>
              <th>Ações</th>
            </tr>
          </thead>
          <tbody>
            <% @admins.each do |admin| %>
              <tr>
                <td> <%= admin.id %> </td>
                <td> <%= admin.email %> </td>
                <td> <%= admin.created_at %> </td>
                <td> <%= admin.updated_at %> </td>
                <td>
                  <%= link_to edit_admins_admin_path(admin),
                      class: "btn btn-primary btn-xs btn-green-alt" do %>
                      <i class="fas fa-edit"></i>
                      Editar
                  <% end %>
                </td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
